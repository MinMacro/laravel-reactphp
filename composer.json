{
    "name": "shakahl/laravel-reactphp",
    "type": "library",
    "description": "ReactPHP Server for Laravel 5.3 and Lumen 5.3",
    "keywords": ["ReactPHP", "Laravel", "Lumen", "server"],
    "homepage": "http://github.com/shakahl/laravel-reactphp",
    "license": "MIT",
    "authors": [
        {
            "name": "Soma Szélpál",
            "email": "szelpalsoma@gmail.com",
            "homepage": "https://github.com/shakahl"
        },
        {
            "name": "Takuya Sato",
            "email": "takuya0219@gmail.com",
            "homepage": "https://github.com/nazo"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "react/react": "0.4.*",
        "guzzlehttp/guzzle": "~6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelReactPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelReactPHP\\Tests\\": "tests/"
        }
    }
